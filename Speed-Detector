
 function speedDetector(speed) {
    const limit=70 ;
    if (speed <= limit )
    return `ok`;
    else {
        let points = Math.floor((speed-limit)/5)
        if(points <=12)
        return`points:${points}`;
        else{
            return "lisence expired!!"
        }

    }
}
console.log(speedDetector(139))